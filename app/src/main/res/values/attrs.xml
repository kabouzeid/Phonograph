<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="roundSelector" format="reference" />
    <attr name="rectSelector" format="reference" />
    <attr name="rectSelectorStrong" format="reference" />

    <attr name="defaultFooterColor" format="color" />

    <attr name="toolbarPopupTheme" format="reference" />

    <attr name="iconColor" format="color" />
    <attr name="dividerColor" format="color" />

    <!-- Setting Views. -->
    <declare-styleable name="TouchInterceptFrameLayout">
        <!-- Sets the TouchInterceptHorizontalScrollView for the TouchInterceptFrameLayout.
        NOT OPTIONAL
        Must be set here, however can be set programmatically.-->
        <attr name="setTouchInterceptHorizontalScrollView" format="reference"/>
        <!-- Sets the TextView for the TouchInterceptFrameLayout. Must be a child of the set TouchInterceptHorizontalScrollView
        NOT OPTIONAL
        Must be set here, however can be set programmatically.-->
        <attr name="setScrollableTextView" format="reference"/>
        <!-- Sets the List Parent that is associated with this view. Must be a ListView or a RecyclerView.
         No need to set if this view never interacts with a ListParent.
         Can be set here or programmatically-->
        <attr name="setListParent" format="reference"/>
    </declare-styleable>

</resources>